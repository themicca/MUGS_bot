<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net9.0</TargetFramework>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <UserSecretsId>017ca7cf-3bf3-4044-adb6-9c9f580e10c4</UserSecretsId>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="CsvHelper" Version="33.1.0" />
    <PackageReference Include="Microsoft.Extensions.Hosting" Version="9.0.9" />
    <PackageReference Include="NetCord" Version="1.0.0-alpha.432" />
    <PackageReference Include="NetCord.Hosting" Version="1.0.0-alpha.432" />
    <PackageReference Include="NetCord.Hosting.AspNetCore" Version="1.0.0-alpha.432" />
    <PackageReference Include="NetCord.Hosting.Services" Version="1.0.0-alpha.432" />
    <PackageReference Include="NetCord.Services" Version="1.0.0-alpha.432" />
  </ItemGroup>

  <ItemGroup>
    <None Update="appsettings.json">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </None>
  </ItemGroup>

</Project>
